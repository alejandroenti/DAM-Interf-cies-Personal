<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane maxWidth="-Infinity" prefHeight="30.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.ControllerScreen">
   <children>
      <HBox fx:id="layoutScreen" prefWidth="530.0" style="-fx-fill-height: True;">
         <children>
            <ImageView fx:id="imageScreen" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="textScreen" style="-fx-background-color: #5ED6BA; -fx-background-radius: 8;" text="Label" wrapText="true">
               <HBox.margin>
                  <Insets left="16.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
